/* Add ccastelo 20150501 */
#sddm
{	margin: 0;
	padding: 0;
	z-index: 30}

#sddm li
{	margin: 0;
	padding: 0;
	list-style: none;
	float: left;
	font: normal 11px arial;}

#sddm li a
{	display: block;
	margin: 0px auto;
	padding: 4px 5px;
	color: #000;
	text-decoration: none;
	vertical-align: top; 
}

#sddm li a img
{	border:0px; 
}


#sddm li a:hover
{	background: #FFF;
	border: 1px solid #000;
	padding: 3px 4px;
}

#sddm div
{	position: absolute;
	visibility: hidden;
	margin: 0;
	padding: 0;
	border: 1px solid #5970B2;
	background-color:#fff;}

#sddm div li
{	position: relative;
	display: block;
	margin: 0;
	padding: 4px 5px;
	width: auto;
	white-space: nowrap;
	text-align: left;
	text-decoration: none;
	color: #2875DE;
	font: 11px arial;
	vertical-align: middle;}

#sddm .bar
{	vertical-align: bottom;
	font-size: 16px;
	color: #444;
	margin-top: 4px;
	}

#sddm legend {
	background-color: #3376AB;
	height: 16px;
	width: 100%;
	color: #fff;
	padding: 7px 0px;
	text-align: center;
	vertical-align: middle;
}
/* End ccastelo 20150501 */